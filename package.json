    "react-popover": "0.5.2",
    "react-popover-fork": "^0.6.4",
    "react-redux": "^5.0.1",
    "react-select": "1.0.0-rc.10",
    "redux": "^3.5.1",
    "redux-undo": "^1.0.0-beta8",
    "reselect": "3.0.1",
    "soundfont-player": "^0.10.4",
    "xmldoc": "^0.5.1"
  }
